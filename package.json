{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "gulp-jshint",
    "description": "api documentation for [gulp-jshint (v2.0.4)](http://github.com/spalger/gulp-jshint) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-gulp-jshint.svg)](https://travis-ci.org/npmdoc/node-npmdoc-gulp-jshint)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "gulp-jshint": "*",
        "jshint": "2.x",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-gulp-jshint",
    "keywords": [
        "documentation",
        "gulp-jshint"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_gulp_jshint.js",
    "name": "npmdoc-gulp-jshint",
    "nameAlias": "npmdoc_gulp_jshint",
    "nameOriginal": "npmdoc-gulp-jshint",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-gulp-jshint.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
}
